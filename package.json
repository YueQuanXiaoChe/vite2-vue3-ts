{
  "name": "vite2-vue3-ts",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",

    "report": "cross-env REPORT=true BUILD_ENV=dev npm run build",

    "build:dev":    "cross-env BUILD_ENV=dev    npm run build",
    "build:test":   "cross-env BUILD_ENV=test   npm run build",
    "build:stress": "cross-env BUILD_ENV=stress npm run build",
    "build:pre":    "cross-env BUILD_ENV=pre    npm run build",
    "build:prod":   "cross-env BUILD_ENV=prod   npm run build",

    "docker-build:dev":    "docker build --build-arg ENV=dev .    -t tcsl-mobile-app-dev",
    "docker-build:test":   "docker build --build-arg ENV=test .   -t tcsl-mobile-app-test",
    "docker-build:stress": "docker build --build-arg ENV=stress . -t tcsl-mobile-app-stress",
    "docker-build:pre":    "docker build --build-arg ENV=pre .    -t tcsl-mobile-app-pre",
    "docker-build:prod":   "docker build --build-arg ENV=prod .   -t tcsl-mobile-app-prod",

    "docker-run:dev":    "npm run docker-build:dev    && docker run -d -p 3000:80 tcsl-mobile-app-dev",
    "docker-run:test":   "npm run docker-build:test   && docker run -d -p 3000:80 tcsl-mobile-app-test",
    "docker-run:stress": "npm run docker-build:stress && docker run -d -p 3000:80 tcsl-mobile-app-stress",
    "docker-run:pre":    "npm run docker-build:pre    && docker run -d -p 3000:80 tcsl-mobile-app-pre",
    "docker-run:prod":   "npm run docker-build:prod   && docker run -d -p 3000:80 tcsl-mobile-app-prod"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "lodash-es": "^4.17.21",
    "vant": "^3.0.7",
    "vue": "^3.0.5",
    "vue-router": "^4.0.4",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@types/http-proxy": "^1.17.5",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^14.14.32",
    "@types/qs": "^6.9.6",
    "@types/rollup-plugin-visualizer": "^2.6.0",
    "@vitejs/plugin-vue": "^1.1.4",
    "@vitejs/plugin-vue-jsx": "^1.1.2",
    "@vue/compiler-sfc": "^3.0.5",
    "autoprefixer": "^10.2.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "http-proxy": "^1.18.1",
    "less": "^4.1.1",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^2.2.1",
    "rollup-plugin-visualizer": "^4.2.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3",
    "vite": "^2.0.1",
    "vite-plugin-compression": "^0.2.3",
    "vite-plugin-html": "^2.0.3",
    "vite-plugin-imagemin": "^0.2.9",
    "vite-plugin-style-import": "^0.7.6"
  },
  "resolutions": {
    "//": "Used to install imagemin dependencies, because imagemin may not be installed in China.If it is abroad, you can delete it",
    "bin-wrapper": "npm:bin-wrapper-china"
  }
}
